# User physical database credentials
MONGO_PHYSICAL_DB_URI_TEMPLATE=mongodb://{username}:{password}@localhost/{dbName}
MYSQL_PHYSICAL_DB_URI_TEMPLATE=mysql://{user}:{password}@{host}/{dbName}
