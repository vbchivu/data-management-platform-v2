MONGO_APP_VIRTUAL_DB_URI=mongodb://<virtual_db_admin>:<password>@localhost:27017/<application-virtual-db>
MYSQL_PHYSICAL_DB_ID=<physical_db_id>

MONGO_PORT=27017
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=
MYSQL_PASSWORD=
